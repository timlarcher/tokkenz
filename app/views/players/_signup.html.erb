<%= form_tag '/signup' do %>

<p>First name: <%= text_field "player", "first_name" %> </p>
<p>Last name: <%= text_field "player", "last_name" %> </p>
<p>User name: <%= text_field "player", "username" %> </p>
<p>Password: <%= password_field "player", "password" %> </p>
<p>Email: <%= text_field "player", "email" %> </p>

<p><%= submit_tag 'Sign up' %></p>
<% end %>
